import { MarkedOptions, I_Marked, MarkedExtension } from '../utssdk/interface';
import { Marked } from './Instance';

export const useMarked = (options?: MarkedOptions): I_Marked => {
	const instance = new Marked();
	if (options) {
		// 应用配置选项
		instance.use(options);
	}
	return instance;
}

export * from './Instance';