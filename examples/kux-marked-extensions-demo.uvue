	// åˆ›å»º Lexer å®ä¾‹
	const lexer = new Lexer();
	
	// è·å–æ‰©å±•æ³¨å†Œå™¨
	const extensionRegistry = lexer.getExtensionRegistry();
	
	// è·å–æ‰©å±•ç»Ÿè®¡ä¿¡æ¯
	const extensionStats = extensionRegistry.getStats();
	
	// è·å–å·²æ³¨å†Œçš„æ‰©å±•åç§°
	const registeredExtensions = extensionRegistry.getRegisteredNames();
	
	// åœ¨é¡µé¢åŠ è½½æ—¶è¾“å‡ºæ‰©å±•ä¿¡æ¯åˆ°æ§åˆ¶å°
	onMounted(() => {
